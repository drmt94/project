<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>


   <class name="com.project.Models.Call" table="callrecords">
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="time" column="time" type="string"/>
        <property name="callState" column="callState" type="string"/>
        <property name="duration" column="duration" type="int"/>
        <property name="phoneNumber" column="phoneNumber" type="string"/>
        <property name="savedContact" column="savedContact" type="boolean"/>
    </class>



</hibernate-mapping>
